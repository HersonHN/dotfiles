#!/usr/bin/env bash

# Git push and add message

if [[ $(git diff | grep "^\+" | grep "console.log") ]]; then
    echo "The commit has lines with console.log"
    echo "use git diff to check"
else
    git_branch=$(git branch | grep '*' | tr '* ' '\0')
    git add -A && git commit -m "$*" && git push origin $git_branch
fi

